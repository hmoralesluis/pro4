<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link href="{{ asset('bundles/front/css/css.css') }}" rel="stylesheet" type="text/css">
    <!-- Libs CSS -->
    <link rel="stylesheet" href="{{ asset('bundles/front/css/bootstrap.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/front/css/fonts/font-awesome/css/font-awesome.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/front/vendor/flexslider/flexslider.css') }}" media="screen"/>
    <link rel="stylesheet" href="{{ asset('bundles/front/vendor/fancybox/jquery.fancybox.css') }}" media="screen"/>
    <!-- Theme CSS -->
    <link rel="stylesheet" href="{{ asset('bundles/front/css/theme.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/front/css/theme-elements.css') }}">
    <!-- Current Page Styles -->
    <link rel="stylesheet" href="{{ asset('bundles/front/vendor/revolution-slider/css/settings.css') }}"
          media="screen"/>
    <link rel="stylesheet" href="{{ asset('bundles/front/vendor/revolution-slider/css/captions.css') }}"
          media="screen"/>
    <link rel="stylesheet" href="{{ asset('bundles/front/vendor/circle-flip-slideshow/css/component.css') }}"
          media="screen"/>
    <!-- Custom CSS -->
    <link rel="stylesheet" href="{{ asset('bundles/front/css/custom.css') }}">
    <!-- Skin -->
    <link rel="stylesheet" href="{{ asset('bundles/front/css/skins/blue.css') }}">
    <!-- Responsive CSS -->
    <link rel="stylesheet" href="{{ asset('bundles/front/css/bootstrap-responsive.css') }}"/>
    <link rel="stylesheet" href="{{ asset('bundles/front/css/theme-responsive.css') }}"/>
    <!-- Favicons -->
    <link rel="shortcut icon" href="{{ asset('bundles/front/img/favicon.ico') }}">
    <link rel="apple-touch-icon" href="{{ asset('bundles/front/img/apple-touch-icon.png') }}">
    <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('bundles/front/img/apple-touch-icon-72x72.png') }}">
    <link rel="apple-touch-icon" sizes="114x114" href="{{ asset('bundles/front/img/apple-touch-icon-114x114.png') }}">
    <link rel="apple-touch-icon" sizes="144x144" href="{{ asset('bundles/front/img/apple-touch-icon-144x144.png') }}">
    <!-- Head Libs -->
    <script src="{{ asset('bundles/front/vendor/modernizr.js') }}"></script>
    {% block stylesheets %}{% endblock %}
</head>
<body>
<div class="body">
    <header>
        <div class="container">
            {% block header %}
                {% block options %}{% endblock %}
            {% endblock %}
        </div>
    </header>
    {% block body %}{% endblock %}
    {% block footer %}{% endblock %}
</div>
<script src="{{ asset('bundles/front/vendor/jquery.min.js') }}"></script>
<script> window.jQuery || document.write('<script src="{{ asset('bundles/front/vendor/jquery.js') }}"><\/script>') </script>
<script src="{{ asset('bundles/front/vendor/jquery.easing.js') }}"></script>
<script src="{{ asset('bundles/front/vendor/jquery.cookie.js') }}"></script>
<script src="{{ asset('bundles/front/vendor/bootstrap.js') }}"></script>
<script src="{{ asset('bundles/front/vendor/selectnav.js') }}"></script>
<script src="{{ asset('bundles/front/vendor/twitterjs/twitter.js') }}"></script>
<script src="{{ asset('bundles/front/vendor/revolution-slider/js/jquery.themepunch.plugins.js') }}"></script>
<script src="{{ asset('bundles/front/vendor/revolution-slider/js/jquery.themepunch.revolution.js') }}"></script>
<script src="{{ asset('bundles/front/vendor/flexslider/jquery.flexslider.js') }}"></script>
<script src="{{ asset('bundles/front/vendor/circle-flip-slideshow/js/jquery.flipshow.js') }}"></script>
<script src="{{ asset('bundles/front/vendor/fancybox/jquery.fancybox.js') }}"></script>
<script src="{{ asset('bundles/front/vendor/jquery.validate.js') }}"></script>
<script src="{{ asset('bundles/front/js/plugins.js') }}"></script>
<script src="{{ asset('bundles/front/js/views/view.home.js') }}"></script>
<script src="{{ asset('bundles/front/js/theme.js') }}"></script>
<script src="{{ asset('bundles/front/js/custom.js') }}"></script>
{% block javascripts %}{% endblock %}
</body>
</html>
